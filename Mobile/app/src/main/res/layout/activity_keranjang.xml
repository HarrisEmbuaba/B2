    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/kembali"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/fkerxconsbottom"
        app:layout_constraintBottom_toBottomOf="parent">
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/fkerxrecyclekeranjang"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            />
    </ScrollView>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/fkerxconsbottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="#FCF3EE">
        <CheckBox
            android:id="@+id/fkerxcheckboxall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@id/fkerxbtnbeli"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/fkerxcheckboxall"
            app:layout_constraintBottom_toTopOf="@id/fkerxbtnbeli"
            android:text="Pilih Semua"
            android:textSize="20dp"
            android:textColor="@color/black"/>
        <TextView
            android:id="@+id/dataxhargabelitotal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@id/fkerxbtnbeli"
            android:text="Rp. Gratis"
            android:textSize="20dp"
            android:textColor="@color/black"
            android:textStyle="bold"/>
        <Button
            android:id="@+id/fkerxbtnbeli"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/coklat"
            android:text="Chekout"
            android:textSize="20dp"
            app:cornerRadius="10dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

